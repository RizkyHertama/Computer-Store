@model IEnumerable<Computer_Store.Models.Pembayaran>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.JenisPembayaran)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.JenisPembayaran)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDPembayaran }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDPembayaran }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDPembayaran })
        </td>
    </tr>
}

</table>
